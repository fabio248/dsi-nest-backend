{
  "name": "dsi-nest-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --config ./test/jest-e2e.json ",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "prisma:migrate:save": "npx prisma migrate dev --preview-feature --skip-seed --create-only",
    "prisma:migrate:update": "npx prisma migrate dev --skip-seed",
    "prisma:migrate:run": "npx prisma migrate deploy --preview-feature"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.367.0",
    "@aws-sdk/s3-request-presigner": "^3.367.0",
    "@dinero.js/currencies": "2.0.0-alpha.1",
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/swagger": "7.0.12",
    "@prisma/client": "4.16.2",
    "@types/date-fns": "^2.6.0",
    "axios": "1.6.0",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "date-fns": "^2.30.0",
    "dinero.js": "^2.0.0-alpha.14",
    "handlebars": "4.7.7",
    "nestjs-pino": "3.3.0",
    "nodemailer": "^6.9.3",
    "passport": "0.6.0",
    "pdf-lib": "^1.17.1",
    "pdfkit-table": "^0.1.99",
    "pino-http": "8.3.3",
    "pino-pretty": "10.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "9.5.0",
    "@nestjs/schematics": "9.1.0",
    "@nestjs/testing": "^10.3.10",
    "@types/axios": "0.14.0",
    "@types/bcryptjs": "2.4.2",
    "@types/dinero.js": "1.9.2",
    "@types/express": "4.17.13",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
    "@types/nodemailer": "^6.4.8",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "2.0.11",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "29.1.0",
    "jest-mock-extended": "^3.0.4",
    "passport-jwt": "^4.0.1",
    "prettier": "2.3.2",
    "prisma": "^4.16.2",
    "source-map-support": "0.5.20",
    "supertest": "6.1.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.entity.(t|j)s",
      "!**/*.exception.(t|j)s",
      "!**/*.input.(t|j)s",
      "!**/*.guard.(t|j)s",
      "!**/*.strategy.(t|j)s",
      "!**/*.arg.(t|j)s",
      "!**/*.module.(t|j)s",
      "!**/*.controller.(t|j)s",
      "!**/*.response.(t|j)s",
      "!**/*.decorator.(t|j)s",
      "!**/main.ts",
      "!**/base-entity.ts",
      "!**/typeorm.service.ts",
      "!**/index.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
